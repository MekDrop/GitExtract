{
  "name": "git-extract",
  "productName": "Git Extract",
  "description": "GUI tool for extract some files with history to another repository",
  "version": "0.0.1",
  "private": true,
  "author": "Raimondas Rimkeviƒçius <github@mekdrop.name>",
  "main": "app/background.js",
  "build": {
    "appId": "com.git-extract.electron",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "start": "node build/start.js",
    "release": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder"
  },
  "dependencies": {
    "chdir": "^0.0.0",
    "command-exists": "^1.2.2",
    "event-abstract-class": "^1.0.6",
    "fs-jetpack": "^1.3.0",
    "git-url-parse": "^8.1.0",
    "homedir": "^0.6.0",
    "is-online": "^7.0.0",
    "is-windows": "^1.0.2",
    "jquery": "^3.3.1",
    "jstree": "^3.3.5",
    "progressbar.js": "^1.0.1",
    "replaceall": "^0.1.6",
    "sha1": "^1.1.1",
    "shelljs": "^0.8.1",
    "ssh2-promise": "^0.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.5",
    "@babel/preset-env": "^7.0.0-beta.5",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "chai": "^4.1.0",
    "css-loader": "^0.28.7",
    "electron": "^1.7.11",
    "electron-builder": "^19.43.3",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "source-map-support": "^0.5.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
